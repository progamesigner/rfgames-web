{
  "compilerOptions": {
    "alwaysStrict": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "jsx": "react",
    "module": "ES2020",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "target": "ES2020"
  },
  "exclude": [
    "node_modules"
  ]
}
