    "redux": "^4.0.5",
    "sass": "^1.32.6",
    "sass-loader": "^11.0.0",
    "ts-loader": "^8.0.15",
    "typescript": "^4.1.3",
    "typestyle": "^2.1.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-manifest-plugin": "^3.0.0"
  }
