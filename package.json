    "@types/lz-string": "^1.3.34",
    "@types/mithril": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "clipboard": "^2.0.6",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "csx": "^10.0.2",
    "eslint": "^7.19.0",
    "eslint-plugin-mithril": "^0.2.0",
    "lodash": "^4.17.20",
    "lz-string": "^1.4.4",
    "mini-css-extract-plugin": "^1.3.5",
    "mithril": "^2.0.4",
    "node-fetch": "^2.6.1",
