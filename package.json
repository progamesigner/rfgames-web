    "@typescript-eslint/parser": "^3.0.2",
    "axios": "^0.19.2",
    "clipboard": "^2.0.6",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "eslint": "^7.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-manifest-plugin": "^2.2.0"
  }
