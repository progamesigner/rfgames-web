{{- $context := 0 | index . -}}

{{- $data := dict -}}

{{- $professions := slice $context "/v2/professions" | partial "functions/gw2-api-paginated-data.dict" -}}

{{- range $professions -}}
    {{- $id := .id | string -}}
    {{- $data = . | dict $id | merge $data -}}
{{- end -}}

{{- return $data -}}
