{{- $context := 0 | index . -}}
{{- $type := 1 | index . -}}
{{- $query := 2 | index . -}}

{{- $name := $query -}}
{{- $slug := $query | slice $context | partial "functions/gw2-data-slugify.string" -}}

{{- $slugToName := dict -}}

{{- if $type | eq "skill" -}}
    {{- $slugToName = slice $context | partialCached "functions/gw2-data-skill-slug-to-name.dict" -}}
{{- end -}}

{{- $name = $slug | index $slugToName | default $query | string -}}

{{- return $name -}}
