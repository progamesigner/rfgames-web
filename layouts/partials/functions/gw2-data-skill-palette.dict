{{- $context := 0 | index . -}}

{{- $data := dict -}}

{{- $professions := slice $context | partialCached "functions/gw2-api-get-professions.dict" -}}

{{- range $professions -}}
    {{- range .skills_by_palette -}}
        {{- $paletteId := 0 | index . -}}
        {{- $skillId := 1 | index . | string -}}
        {{- $data = dict $skillId $paletteId | merge $data -}}
    {{- end -}}
{{- end -}}

{{- return $data -}}
