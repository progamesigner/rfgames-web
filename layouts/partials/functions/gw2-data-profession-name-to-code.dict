{{- $context := 0 | index . -}}

{{- $data := slice $context "profession-name-to-code" | partial "functions/gw2-data-load.dict" -}}

{{- if $data | len | ge 0 -}}
    {{- $professions := slice $context | partialCached "functions/gw2-api-get-professions.dict" -}}

    {{- range $professions -}}
        {{- $professionName := "name" | index | lower -}}
        {{- $data = "code" | index . | dict $professionName | merge $data -}}
    {{- end -}}
{{- end -}}

{{- return $data -}}
