{{- $context := 0 | index . -}}

{{- $professions := slice $context | partialCached "functions/gw2-api-get-professions.dict" -}}

{{- $data := dict -}}
{{- range $professions -}}
    {{- $professionName := .name | lower -}}
    {{- $data = .code | dict $professionName | merge $data -}}
{{- end -}}

{{- return $data -}}
