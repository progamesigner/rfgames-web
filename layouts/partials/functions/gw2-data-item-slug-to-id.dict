{{- $context := 0 | index . -}}

{{- $data := dict -}}

{{- $items := slice $context | partialCached "functions/gw2-api-get-items.dict" -}}

{{- range $items -}}
    {{- $slug := slice $context .name | partial "functions/gw2-data-slugify.string" -}}
    {{- if $slug | index $data | not -}}
        {{- $data = .id | dict $slug | merge $data -}}
    {{- end -}}
{{- end -}}

{{- return $data -}}
