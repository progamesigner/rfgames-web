{{- $context := 0 | index . -}}

{{- $data := dict -}}

{{- $traits := slice $context | partialCached "functions/gw2-api-get-traits.dict" -}}

{{- range $traits -}}
    {{- $slug := "name" | index . | slice $context | partial "functions/gw2-data-slugify.string" -}}
    {{- if $slug | index $data | not -}}
        {{- $data = "id" | index . | dict $slug | merge $data -}}
    {{- end -}}
{{- end -}}

{{- return $data -}}
