{{- $context := 0 | index . -}}
{{- $idOrName := 1 | index . -}}

{{- $id := $idOrName -}}
{{- if $id | string | eq $id -}}
    {{- $data := slice $context | partialCached "collections/gw2-item-slugs.dict" -}}
    {{- $slug := $idOrName | slice $context | partial "functions/slugify.string" -}}
    {{- $id = $slug | index $data | int -}}
{{- end -}}

{{- return $id -}}
